<h1>All Contacts</h1>

<table class="table table-striped">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.email %></td>
      <td><a href="/contacts/<%= user.id %>" class="btn btn-info">Show</a></td>
    </tr>
  <% end %>


</table>